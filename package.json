{
  "name": "browserify-templatefy",
  "version": "1.0.5",
  "description": "A Browserify plugin to manage html template with js injection, angular supported",
  "main": "index.js",
  "engines": {
    "node": ">=0.12"
  },
  "bin": {
    "templatefy": "./bin/templatefy"
  },
  "scripts": {
    "test": "rm -rf coverage && node_modules/istanbul/lib/cli.js cover --print=none -i ./lib/**/*.js node_modules/mocha/bin/_mocha -- test/specs.js  && node_modules/istanbul-combine/cli.js -d ./coverage -p both -r lcov -r html ./coverage/coverage.json ./coverage/coverage-**.json",
    "ci-test": "npm test && node_modules/codecov/bin/codecov && node_modules/codeclimate-test-reporter/bin/codeclimate.js < ./coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rubeniskov/browserify-templatefy.git",
    "url_commit": "git+https://github.com/rubeniskov/browserify-templatefy/commit",
    "url_tag": "git+https://github.com/rubeniskov/browserify-templatefy/releases/tag"
  },
  "keywords": [
    "template",
    "html",
    "angular",
    "browserify",
    "templatefy"
  ],
  "author": {
    "name": "Rubén López Gómez",
    "email": "me@rubeniskov.com",
    "url": "https://twitter.com/Rubeniskov"
  },
  "licenses": [
    {
      "type": "WTFPL",
      "url": "https://github.com/rubeniskov/browserify-templatefy/blob/master/LICENSE.md"
    }
  ],
  "bugs": {
    "url": "https://github.com/rubeniskov/browserify-templatefy/issues"
  },
  "homepage": "http://templatefy.rubeniskov.com",
  "dependencies": {
    "browserify-transform-tools": "^1.6.0",
    "inherify": "0.0.2",
    "nomnom": "^1.8.1",
    "optiopus": "^0.1.3",
    "templatefy": "^0.1.7"
  },
  "peerDependencies": {
    "browserify": ">= 2.3.0 < 14"
  },
  "devDependencies": {
    "browserify": "13.0.1",
    "chai": "^3.5.0",
    "codeclimate-test-reporter": "^0.3.1",
    "codecov": "^1.0.1",
    "grunt": "^1.0.1",
    "grunt-bump": "^0.8.0",
    "grunt-npm": "0.0.2",
    "istanbul": "^0.4.3",
    "istanbul-combine": "^0.3.0",
    "mocha": "^2.4.5"
  }
}
